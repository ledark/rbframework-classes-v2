{
    "name": "ledark/rbframeworks2",
    "type": "library",
    "description": "Utilitarios para RBFrameworks otimizado para PHP 8.4+",
    "keywords": [
        "rbframeworks"
    ],
    "homepage": "https://github.com/ledark/rbframework-classes-v2",
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Bermejo",
            "email": "mestreledark@gmail.com",
            "homepage": "http://bermejo.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.4.0",
        "sergeytsalkov/meekrodb": "^3.1",
        "eftec/bladeone": "^4.15",
        "bramus/router": "^1.6",
        "symfony/cache": "^7.1",
        "guzzlehttp/guzzle": "^7.9"
    },
    "autoload": {
        "psr-4": {
            "RBFrameworks\\": [
                "src/",
                "tests/"
            ],
        },
        "files": [
            "_include.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/console": "^7.1",
        "chrome-php/chrome": "^1.11"      
    }
}
